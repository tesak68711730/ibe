{
  "name": "ibefront-end",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf -- node_modules doc coverage dist dev",
    "clean:install": "yarn run clean && yarn install",
    "clean:start": "yarn run clean && yarn start",
    "clean:dist": "rimraf -- dist dev",
    "build": "yarn run clean:dist && yarn run ngc && webpack --config config/webpack.dev.js --progress --profile --bail",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --hot --watch --content-base src/",
    "server:prod": "node server.js",
    "test": "yarn run clean:dist && karma start",
    "test:watch": "yarn run clean:dist && karma start --reporters dots,notify --single-run false",
    "docs": "typedoc --options typedoc.json  src/**/*.ts",
    "start": "yarn run server:dev",
    "ngc": "ngc -p tsconfig.json --locale=en-US",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/cdk": "^5.1.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.1.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "@progress/kendo-angular-buttons": "^2.0.0",
    "@progress/kendo-angular-l10n": "^1.0.5",
    "@progress/kendo-angular-layout": "^1.2.0",
    "@progress/kendo-theme-default": "^2.46.0",
    "aphrodite": "^1.2.5",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^2.1.0",
    "lodash-es": "^4.16.6",
    "primeng": "^2.0.4",
    "rxjs": "^5.5.2",
    "systemjs": "0.19.39",
    "time-ago": "^0.1.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/platform-server": "~2.4.8",
    "@types/jasmine": "~2.5.53",
    "@types/lodash": "^4.14.38",
    "@types/node": "^7.0.0",
    "@types/pluralize": "^0.0.27",
    "@types/source-map": "^0.5.0",
    "@types/webpack": "^2.0.0",
    "angular2-router-loader": "^0.3.3",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "codelyzer": "^4.0.1",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-proxy": "^1.15.2",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
